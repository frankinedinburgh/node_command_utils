{
  "name": "cli",
  "version": "1.0.0",
  "description": "app for tracking repositoies",
  "main": "index.js",
  "scripts": {
    "fetch:tickets:progress": "QUERY='assignee=frank and status in(\"Code Test\")' DEST=./commands/ node ./commands/fetch_tickets",
    "fetch:tickets:all": "QUERY='assignee=frank' DEST=./commands/ node ./commands/fetch_tickets",
    "move:tickets": "cp commands/tickets.json ~/Sites/041018/src/db/",
    "commit:time": "DIR=~/Sites/dugout-mena-v1/ node ./playground/fetch",
    "commit:version": "DIR=~/Sites/dugout-mena-v1 node ./commands/track-git-version",
    "deploy:sandbox": "DIR=~/Sites/dugout-mena-v1 LOCATION=sandbox node ./commands/deploy",
    "deploy:frank": "DIR=~/Sites/dugout-mena-v1 LOCATION=frank node ./commands/deploy",
    "mail": "DIR=~/Sites/dugout-mena-v1 SUBJECT='DUGOUT MENA V1 JWPLAYER deployed' node ./commands/mail",
    "mail:all": "DIR=~/Sites/dugout-mena-v1 SUBJECT='DUGOUT MENA V1 deployment' CC=all node ./commands/mail",
    "tickets": "npm-run-all --serial fetch:tickets:progress move:tickets"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "lodash": "^4.17.11",
    "nodemailer": "^4.7.0",
    "pug": "^2.0.3",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
